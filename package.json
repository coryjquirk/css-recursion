{
  "name": "css-recursion",
  "version": "1.0.0",
  "description": "graphics playground",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "cory j. quirk",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.1.13",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "fontawesome": "^5.6.3",
    "if-env": "^1.0.4",
    "semver": "^7.3.4"
  },
  "engines": {
    "node": "12.18.3"
  },
  "main": "script.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coryjquirk/css-recursion.git"
  },
  "bugs": {
    "url": "https://github.com/coryjquirk/css-recursion/issues"
  },
  "homepage": "https://github.com/coryjquirk/css-recursion#readme"
}
